.content{
    padding:24rpx;
    padding-bottom:80rpx;
}
.atitles{
    text-align:left;
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:2;
    overflow: hidden;
    text-overflow:ellipsis;
    color:#333;
    font-size:40rpx;
}
.recenta{
    color:#333;
    font-size:32rpx;
    margin-top:40rpx;
}
.alls{
    margin-top:16rpx;
    color:#f48403;
    font-size:32rpx;
}
.buttons{
    height:66rpx;
    padding:15rpx 44rpx;
    line-height:66rpx;
    border-top:2rpx solid #f5f5f5;
}
.collect{
    padding-right:60rpx;
    border-right:2rpx solid #c2c2c2;
    display:inline-block;
}
.collect image{
    width:44rpx;
    height:44rpx;
    display:inline-block;
    margin-top:11rpx;
    float:left;
}
.collect text{
    color:#000;
    font-size:32rpx;
    height:66rpx;
    line-height:66rpx;
    display:inline-block;
    margin-left:10rpx;
}
.join{
    display:inline-block;
    color:#000;
    font-size:32rpx;
    height:66rpx;
    line-height:66rpx;
    margin-left:175rpx;
}

.atime{
    margin:0 auto;
    color:#333;
    font-size:32rpx;
    overflow: hidden;
    border-bottom: 2rpx solid #f5f5f5;
    height:80rpx;
    line-height:80rpx;
    margin-top:40rpx;
}
.atime text{
    float:left;
    height:80rpx;
    line-height:80rpx;
    display:inline-block;
}
.atime .right{
    width:36rpx;
    height:36rpx;
    float:right;
    margin-top:22rpx;
    display:inline-block;
}
.atime .time_icon{
    width:44rpx;
    height:44rpx;
    float:left;
    margin-top:18rpx;
    display:inline-block;
    margin-right:24rpx;
}
.aaddress{
    color:#333;
    font-size:32rpx;
    overflow: hidden;
    border-bottom: 2rpx solid #f5f5f5;
    height:80rpx;
    line-height:80rpx;
}
.aaddress text{
    float:left;
    height:80rpx;
    line-height:80rpx;
    display:inline-block;
}
.aaddress .right{
    width:36rpx;
    height:36rpx;
    float:right;
    margin-top:22rpx;
    display:inline-block;
}
.aaddress .location_icon{
    width:44rpx;
    height:44rpx;
    float:left;
    margin-top:18rpx;
    display:inline-block;
    margin-right:24rpx;
}
.apersons{
    height:75rpx;
    line-height:75rpx;
    color:#333;
    font-size:32rpx;
    padding:16rpx 0;
    border-bottom: 2rpx solid #f5f5f5;
}
.apersons .num{
    margin-right:40rpx;
    display:inline-block;
    float:left;
}
.apersons .aavatar{
    height:75rpx;
    width:75rpx;
    margin-right:5rpx;
    display:inline-block;
    float:left;
}
.apersons .dian{
    display:inline-block;
    float:left;
    color:#999;
    font-size:40rpx;
}
.apersons .right{
    width:36rpx;
    height:36rpx;
    float:right;
    margin-top:22rpx;
    display:inline-block;
}

.basemsg{
    height:112rxp;
    margin:40rpx 0;
    overflow:hidden;
}
.basemsg .img{
    width:112rpx;
    height:112rpx;
    display:block;
    float:left;
    margin-right:16rpx;
}
.basenamecity{
    display:block;
    float:left;
    height:112rpx;
}
.basenamecity .basename{
    color:#333;
    font-size:28rpx;
    margin-top:20rpx;
}
.basenamecity .basecity{
    color:#999;
    font-size:28rpx;
}
.ades{
    color:#666;
    font-size:28rpx;
}
.pictitle{
   color:#333;
   font-size:32rpx;     
   margin:40rpx 0;
}
.apics{
    width:100%;
    margin-bottom:10rpx;
}